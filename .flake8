[flake8]
max-line-length = 100
max-complexity = 20
ignore = 
    # Ignore line length for URLs and long strings
    E501,
    # Allow whitespace before ':'
    E203,
    # Allow line break before binary operator
    W503
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    node_modules,
    .pytest_cache,
    coverage,
    static